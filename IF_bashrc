# handy things for developing at IF

# directories
export IF_ROOT=$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )
export IF_ANGULAR="$IF_ROOT"/app/src
export IF_SCHEMAS="$IF_ROOT"/components/IF_schemas
export IF_ANALYTICS="$IF_ROOT"/IF_services/IF_analytics

# fun aliases
alias wow='git status'
alias such='git'
alias very='git'
alias much='git'

alias doge='cat'

alias up='cd ..'

# typos
alias ndoe='node'



export TESTDB='localhost:37017'



