<?xml version='1.0' encoding='utf-8'?>
<widget id="com.if.bubblli" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bubbl.li</name>
    <description>
        Internet of Places.
    </description>
    <author email="hello@interfacefoundry.com" href="http://interfacefoundry.com">
        Interface Foundry
    </author>
    <content src="index.html" />
    <access origin="*" />
    
    <feature name="EstimoteBeacons">
    	<param name="ios-package" value="EstimoteBeacons" />
    	<param name="onload" value="true" />
	</feature>
	
	<preference name="UIWebViewBounce" value="false" />
	<preference name="EnableLocation" value="true" />
	
	<preference name="DisallowOverscroll" value="true" />
	<preference name="webviewbounce" value="false" />
	
	<preference name="AutoHideSplashScreen" value="false" />
	
	<plugin name="Geolocation" value="CDVLocation" />
</widget>
