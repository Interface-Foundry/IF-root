{
  "apps": [{
    "name": "mint",
    "cwd": ".",
    "script": "./server/index.js",
    "combine_logs": true,
    "log_date_format" : "YYYY-MM-DD HH:mm Z",
    "watch": false,
    "env": {
      "NODE_ENV": "production",
      "MONGODB_URI": "mongodb://mongodb-beta-server-1:27017,mongodb-beta-server-2:27017/mint?replicaSet=rs0&readPreference=nearest&slaveOk=true",
      "SEND_EMAILS": true,
      "NO_LIVE_RELOAD": true,
      "NO_LUMINATI": true,
      "BASEURL": "https://www.kipthis.com",
      "LOGGING_MODE": "database",
      "LOGGING_LEVEL": "info",
      "BUILD_MODE": "prebuilt",
      "SENDGRID_WEBHOOK": "https://kipthis.com/sg/",
      "YPO_ENABLED": true
    }
  }]
}
